{
    "name": "root/html",
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpmd/phpmd" : "@stable",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "phpcs": "./vendor/bin/phpcs --standard=phpcs.xml",
