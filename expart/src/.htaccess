RewriteEngine On

RewriteCond %{REQUEST_URI} !^/poll
RewriteRule .? /poll/index.php [R,L]

# RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /poll/$1/index.php

